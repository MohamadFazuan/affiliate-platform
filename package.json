{
  "name": "affiliate-intelligence-platform",
  "version": "1.0.0",
  "description": "Social Media Affiliate Product Selection & Tracking Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker:dev": "wrangler dev backend/worker.ts",
    "worker:deploy": "wrangler deploy backend/worker.ts",
    "db:migrate": "wrangler d1 execute affiliate-db --local --file=database/schema.sql",
    "db:migrate:remote": "wrangler d1 execute affiliate-db --remote --file=database/schema.sql",
    "db:seed": "wrangler d1 execute affiliate-db --local --file=database/seed.sql",
    "db:seed:remote": "wrangler d1 execute affiliate-db --remote --file=database/seed.sql",
    "pages:deploy": "npm run build && wrangler pages deploy .next",
    "clean": "rm -rf .next node_modules"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^8.7.0",
    "bcrypt-ts": "^8.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.3.0",
    "lucide-react": "^0.363.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.71.1",
    "recharts": "^2.12.0",
    "stripe": "^20.3.1",
    "tailwind-merge": "^2.6.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "wrangler": "^4.64.0"
  }
}
